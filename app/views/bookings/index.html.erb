
<div class="d-flex justify-content-around"><h3>Items</h3><h3>Start Date</h3><h3>End Date</h3></div>
  <div class = "d-flex justify-content-center" style="margin: 5rem" >
    <div class="card" id="div-booking" style="margin-top: -5rem ; width: 65rem">
      <ul class="list-group list-group-flush">
        <% @bookings.each do |booking|%>
          <li class="list-group-item">
            <div class="d-flex justify-content-between" style= "height: 3rem">
            <div><%= booking.item.name %></div>
            <div><%= booking.start_date.day %>/0<%= booking.start_date.month%>/
            <%= booking.start_date.year %></div>
            <div><%= booking.end_date.day%>/0<%= booking.end_date.month%>/
            <%= booking.end_date.year%> </div>
            </div>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
